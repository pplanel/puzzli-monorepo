    "@stencil/sass": "^1.4.1",
    "@storybook/addon-actions": "^6.1.19",
    "@storybook/addon-essentials": "^6.1.19",
    "@storybook/addon-links": "^6.1.19",
    "@storybook/addon-notes": "^5.3.21",
