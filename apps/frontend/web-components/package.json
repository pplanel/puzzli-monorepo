{
  "name": "web-components",
  "version": "1.0.0",
  "description": "Stencil Web Components with Storybook",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "collection": "./dist/collection/collection-manifest.json",
  "es2015": "./dist/esm-es5/index.mjs",
  "es2017": "./dist/esm/index.mjs",
  "collection:main": "./dist/collection/index.js",
  "unpkg": "./dist/web-components/web-components.js",
  "private": true,
  "scripts": {
    "build": "stencil build --docs",
    "dev": "stencil build --docs --watch",
    "start": "run-p dev storybook",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006 -s ./dist",
    "build-storybook": "build-storybook -c ./.storybook -o ../public"
  },
  "dependencies": {
    "puppeteer": "^5.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@stencil/core": "^1.17.2",
    "@stencil/sass": "^1.4.1",
    "@storybook/addon-essentials": "^6.1.19",
    "@storybook/web-components": "^6.1.21",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.35",
    "@types/puppeteer": "^5.4.2",
    "case": "^1.6.3",
    "chokidar": "^3.5.1",
    "copy-webpack-plugin": "^8.0.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "storybook-addon-jsx": "^7.3.6",
    "webpack": "^4.46.0",
    "write-file-webpack-plugin": "^4.5.1",
    "puzzl-components": "ssh://git@github.com:puzzlsoftwarehouse/puzzl-components.git#0.0.5"
  }
}
